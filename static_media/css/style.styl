blacktext = rgba(0, 0, 0, 0.7)
letratext = 'Lato', sans-serif
letratitulo = 'Roboto', sans-serif
colorlink = #d93
negro = #3D3935
verde = #B5BD00

vendor(prop, args)
    -webkit-{prop} args
    -moz-{prop} args
    -o-{prop} args
    {prop} args

transition()
    vendor('transition', arguments)
border-radius()
    vendor('border-radius', arguments)
box-shadow() 
  -moz-box-shadow arguments
  -webkit-box-shadow arguments
  box-shadow arguments


@import url('https://fonts.googleapis.com/css?family=Open+Sans');

body
    font-family: 'Open Sans', sans-serif    
.display-inline
    display inline-block
/*MENU*/
.navbar-default
    box-shadow 0 0 7px rgba(0, 0, 0, 0.3)
    border-radius 0px
    .navbar-nav > li > a, .dropdown-menu li a
        text-transform initial
        font-size 16px !important
        font-weight bold
        font-family letratext
        color blacktext
        &.selected
        &:hover
            color colorlink !important
        
#logo
    font-size 18px
    font-family letratitulo 
    font-weight 500
    color blacktext
ol.breadcrumb li a:hover
    color colorlink
/*Secciones*/

.section-white-xs
    margin-top 20px
    margin-bottom 20px
.section-grey
    padding 30px 0px
h1, h2, h3, h4, h5, h6
    color blacktext
p a
    color colorlink
.titulo-map
    background negro
    padding 10px
    h4
        color #fff
        margin 0px
        padding 10px 10px
        font-size 20px
        display inline-block
    .btn-map
        display inline-block
        margin-bottom 0px
/*Sponsor*/
.sponsors img.sponsor
    opacity 0.8
    height auto
    width 100%
    &:hover
        opacity 1
.sponsors-padding
    padding 17px 0
    border-top 1px solid rgba(61, 57, 53, 0.5)
/*footer*/
.footer, .top-intro
    background negro
a.top-scroll:hover
    background colorlink
    border-color colorlink
ul.footer_social li a i:hover, .footer p a:hover, ul.footer-list li a:hover
    color colorlink

.btn-blue, .btn-blue:focus, .btn-blue:active, .btn-blue-full, .btn-blue-full:focus, .btn-blue-full:active, input.contact-submit
    background colorlink
    border-color colorlink
    padding 10px
.btn-sm.btn-blue:hover, input.contact-submit:hover
    border-color colorlink
    color colorlink
    cursor pointer
    
.footer-top
    padding 20px 0 0px 0
.form-edit-org .group-costum-2
    width 100%
    
/*Registro*/
#contact-form
    max-width 100%
select#input
    color #7c8595

/*consulta*/
.group-costum
    padding 5px 0px
    position inherit
    display block
    .input-group-addon
        background negro
        color white
        line-height normal
        border-radius 0px
        border 0px
    select
        color negro !important
        font-size 16px
        font-weight bold
        padding 5px 12px
.group-costum-2
    padding 5px 0px
    position inherit
    .input-group-addon
        background negro
        color white
        line-height normal
        border-radius 0px
        border 0px
.border-bottom-black
    border-bottom 1px solid rgba(61,57,53,0.5)
    padding-bottom 10px
.pagination > li > a, .pagination > li > span
    color negro
.pagination > .active > a, .pagination > .active > span, .pagination > .active > a:hover, .pagination > .active > span:hover, .pagination > .active > a:focus, .pagination > .active > span:focus
    background-color colorlink
    border-color colorlink
.form-busca
    input.contact-submit
        margin-top 0px
    input[type="text"]
        height 44px
.bootstrap-select:not([class*=col-]):not([class*=form-control]):not(.input-group-btn)
    width 100% !important
.bootstrap-select>.dropdown-toggle
    margin 0px
    padding 5px
.bootstrap-select.btn-group .dropdown-toggle .filter-option
    color negro
.bootstrap-select.btn-group .dropdown-menu li a
    color negro !important
    font-size 16px
.bootstrap-select.btn-group .dropdown-menu li.selected a
    color colorlink !important
.form-edit-org
    label
        font-size 16px
        color negro
    input[type=file]
        display inline-block
.detalle-user
    background-color white
    padding 20px
.reset-pass, .registro-user
    ul
        list-style square
        padding-left 40px
    ul.errorlist
        list-style none
        padding 0
        color red
        li
            padding-left: 1.3em
            &:before
                content 'X'
                font-weight bold
                display inline-block
                margin-left -1.3em
                width 1em
                font-size 17px
.login-user
    margin-bottom 40px
    a 
        color colorlink
.modal-footer .btn + .btn
    margin-bottom 10px
.text-col 
    .list-inline
        min-height 26px
        margin-top 5px
        &:last-child
            margin-bottom 5px
        li
            color #444
            b, i
                color negro
                
#myList .box .text-col p
    color #444
/*Detalle org*/
.obj-org
    margin-bottom 10px
    h5
        margin 5px
.detalle-org .panel-primary, .detalle-user .panel-primary 
    border-color negro
    .panel-heading
        background-color verde
        border-color transparent
        .list-inline > li > a
            color negro
            font-weight bold
    .panel-body
        li
            font-size 16px
            color negro
            a
                color colorlink    
.panel-body a.pull-right
    color negro
    font-weight bold
    i
        padding-right 2px
.lista-perfil li
    margin-bottom 20px
    color negro
    font-weight bold
.lista-perfil>li>a
    color negro
    font-size 17px
#ratelabel
    background-color negro
    color white
    font-size 16px
    font-family: 'Lato', sans-serif;
#input-rate
    max-width 60px
    font-size 16px
    font-family: 'Lato', sans-serif;
    min-height 36px 
#rate-btn
    display inline-block
    font-size 16px
    font-family: 'Lato', sans-serif;
    button
        background-color verde
        color white
        border-color verde
        font-size 16px
        font-family: 'Lato', sans-serif;
        &:hover
            background-color white
            color verde
            border-color verde      
/*buscador*/
.buscador
    position: absolute;
    width: 20%;
    z-index: 99;
    right: 0;
    input.contact-input
        border-radius 0px
        .form-control:focus
            box-shadow inset 0 1px 1px rgba(0,0,0,.075), 0 0 8px rgba(181, 189, 0, 0.5)
.like-btn
    background-color verde
    border-color verde
    &:hover
        color verde !important
        border-color verde !important

.tab-busca
    li a
        font-size 17px
        font-family letratext
        font-weight bold
        color negro
    li.active a
        color colorlink
.endless_page_current
    background-color verde !important
    color white !important
.detalle-org .panel-primary .panel-body li a, .detalle-user .panel-primary .panel-body .pagination  li a
    color negro
/*galeria*/
.popup-gallery:hover img
    opacity 1
.mfp-title
    background-color negro
    text-align center
    padding 10px 0px
    small
        display none !important
.mfp-img-mobile .mfp-bottom-bar
    padding 0px
.no-padding-top
    padding-top 0px
.carousel.carousel1 .carousel-indicators li.active
    background-color colorlink

